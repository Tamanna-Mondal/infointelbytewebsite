<section class="testimonial-form-section py-5 bg-light">
  <div class="container">
    <h2 class="text-center mb-4">Submit Your Testimonial</h2>
    <form id="testimonialForm">
      <div class="row g-3">
        <div class="col-md-6">
          <input type="text" class="form-control" placeholder="Your Name" id="name" required>
        </div>
        <div class="col-md-6">
          <input type="text" class="form-control" placeholder="Your Company (Optional)" id="company">
        </div>
        <div class="col-md-12">
          <textarea class="form-control" rows="4" placeholder="Your Review" id="message" required></textarea>
        </div>
        <div class="col-md-12 text-center">
          <button type="submit" class="btn btn-primary px-4">Submit Review</button>
        </div>
      </div>
    </form>
  </div>
</section>
<section class="new-testimonials mt-4">
